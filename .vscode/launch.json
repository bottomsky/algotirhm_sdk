{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Algo Core Service",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "algo_core_service.main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8000"
            ],
            "envFile": "${workspaceFolder}\\src\\algo_core_service\\.env",
            "justMyCode": true,
            "console": "integratedTerminal",
            "python": "${workspaceFolder}\\.venv\\Scripts\\python.exe"
        }
    ]
}
